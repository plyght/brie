        <string>$(PRODUCT_MODULE_NAME).BrieExtensionHandler</string>
        <key>SFSafariContentScript</key>
        <array>
            <dict>
                <key>Script</key>
                <string>script.js</string>
            </dict>
        </array>
        <key>SFSafariExtensionDisplayName</key>
        <string>Brie Extension</string>
        <key>SFSafariToolbarItem</key>
        <dict>
            <key>Action</key>
            <string>Command</string>
            <key>Identifier</key>
            <string>Button</string>
            <key>Image</key>
            <string>ToolbarItemIcon.pdf</string>
            <key>Label</key>
            <string>Brie</string>
        </dict>
        <key>SFSafariWebsiteAccess</key>
        <dict>
            <key>Level</key>
            <string>All</string>
        </dict>
    </dict>
</dict>
</plist>

